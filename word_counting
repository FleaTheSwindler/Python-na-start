string = "The elephant can pull the heaviest load"
print(string)

words = 1
letters = 0
hash_table = {}


for char in string:
    if char == " ": words +=1
    else: letters +=1

for char in string:
    if char in hash_table: hash_table[char] += 1
    else: hash_table[char] =1


print(words, letters, hash_table)
